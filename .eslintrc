{
    "parser": "babel-eslint",
    "plugins": [
      "react"
    ],
    "rules": {
      "comma-dangle": [1, "always-multiline"],
      "no-underscore-dangle" : 0,
      "max-len": [1, 180, 4],
      "arrow-body-style": [0],
      "react/jsx-no-bind": [0],
      "import/no-unresolved": [0] 
   }
  }